emoji={
    "ğŸ˜Š":"smiling face with smiling eyes",
    "ğŸ‘":"thumbs up",
    "ğŸ™Œ":"raising hands",
    "ğŸ¤":"crossed finger",
    "ğŸ˜’":"unamused face",
    "ğŸ˜‚":"face with tears of joy"
